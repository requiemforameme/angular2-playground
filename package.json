{
  "name": "angular2-primeng-playground",
  "version": "0.0.4",
  "versionName": "Andromeda",
  "description": "Angular 2 playground",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/requiemforameme/angular2-primeng-playground.git"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "copy:bs": "copyfiles -f node_modules/bootstrap/dist/css/bootstrap.min.css assets/css && copyfiles -f node_modules/bootstrap/dist/css/bootstrap-theme.min.css assets/css && copyfiles -f node_modules/bootstrap/dist/fonts/* assets/fonts" 
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-alpha.8",
    "@angular/upgrade": "2.0.0-rc.4",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "copyfiles": "^1.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
